{
  "name": "reactrestaurant",
  "version": "1.0.0",
  "description": "react restaurant site with node and react",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "heroku-postbuild": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mercerbe/ReactRestaurant.git"
  },
  "author": "Ben Mercer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mercerbe/ReactRestaurant/issues"
  },
  "homepage": "https://github.com/mercerbe/ReactRestaurant#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "semantic-ui-react": "^0.82.5",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
